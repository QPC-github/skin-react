{
  "name": "skin-react",
  "version": "1.0.0-beta.2",
  "description": "A set of React Components for Ebay Skin CSS",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run storybook",
    "sanity-check": "yarn lint && yarn build && yarn test",
    "script:update-icons": "node scripts/import-icons-types && yarn lint",
    "build": "npm run clean && tsc && tsc -m es6 --outDir esm --removeComments false && cross-env NODE_ENV=production webpack",
    "build:ci": "yarn lint && yarn coverage && yarn build",
    "version": "npm run build-storybook && npm run build",
    "storybook": "cross-env NODE_ENV=development STORYBOOK=true start-storybook -p 6006 -c .storybook",
    "deploy-storybook": "yarn sanity-check && storybook-to-ghpages",
    "clean": "shx rm -rf lib esm bundles dist",
    "lint": "prettier --write 'src/**/*.{js,jsx,ts,tsx}' '.storybook/**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "coverage": "jest --coverage",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run deploy-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "3",
    "react-focus-lock": "^2.4.1",
    "react-remove-scroll": "^2.3.0",
    "tslib": "^2.0.0",
    "url-loader": "^4.1.0"
  },
  "peerDependencies": {
    "@ebay/skin": "10.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@ebay/skin": "10.6.2",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/core": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/storybook__react": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.13.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^4.2.5",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "jest-transform-css": "^2.0.0",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-assets": "^5.0.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-normalize": "^9.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.6.0",
    "react-highlight": "^0.12.0",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "watch": "^1.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "keywords": [
    "ebay",
    "react",
    "Typescript"
  ],
  "license": "MIT",
  "release": {
    "publishDir": "lib"
  }
}
